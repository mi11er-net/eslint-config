{
  "name": "@mi11er/eslint-config",
  "version": "1.1.0",
  "description": "@mi11er/eslint-config combines various plugins and rulesets for use in mi11er.net projects",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "homepage": "https://github.com/mi11er-net/eslint-config",
  "bugs": {
    "url": "https://github.com/mi11er-net/eslint-config/issues"
  },
  "license": "MIT",
  "author": "Matthew Miller <matthew@mi11er.net> (https://mi11er.net)",
  "contributors": [],
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "index.js",
    "lib/extendedConfigs.js",
    "lib/isInstalled.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mi11er-net/eslint-config.git"
  },
  "scripts": {
    "precommit": "run-s lint-staged pretty-quick",
    "lint": "eslint --fix .",
    "lint-staged": "lint-staged",
    "semantic-release": "semantic-release",
    "pretty-quick": "pretty-quick --staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "1.0.1",
    "@semantic-release/exec": "2.0.1",
    "@semantic-release/git": "3.0.1",
    "babel-eslint": "8.2.1",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.18.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-index": "1.4.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-optimize-regex": "1.1.5",
    "eslint-plugin-react": "7.6.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "0.14.3",
    "lint-staged": "6.1.0",
    "npm-run-all": "4.1.2",
    "prettier": "1.10.2",
    "pretty-quick": "1.4.1",
    "semantic-release": "13.0.1",
    "sort-package-json": "1.7.1"
  },
  "peerDependencies": {
    "babel-eslint": ">=8.2.1",
    "eslint": ">=4.17.0"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/npm",
        "tarballDir": "release"
      },
      {
        "path": "@semantic-release/git",
        "assets": [
          "**"
        ]
      },
      {
        "path": "@semantic-release/github",
        "assets": [
          {
            "path": "release/*.tgz",
            "label": "Package tarball"
          }
        ]
      }
    ]
  }
}
