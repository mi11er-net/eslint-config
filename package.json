{
  "name": "@mi11er/eslint-config",
  "version": "2.2.2",
  "description":
    "@mi11er/eslint-config combines various plugins and rulesets for use in mi11er.net projects",
  "keywords": ["eslint", "eslint-config"],
  "homepage": "https://github.com/mi11er-net/eslint-config",
  "bugs": {
    "url": "https://github.com/mi11er-net/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mi11er-net/eslint-config.git"
  },
  "license": "MIT",
  "author": "Matthew Miller <matthew@mi11er.net> (https://mi11er.net)",
  "contributors": [],
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "index.js",
    "react.js",
    "rules"
  ],
  "main": "index.js",
  "scripts": {
    "precommit": "run-s lint-staged pretty-quick nsp",
    "lint": "eslint --fix .",
    "lint-staged": "lint-staged",
    "nsp": "nsp check",
    "semantic-release": "semantic-release",
    "test": "run-s lint",
    "pretty-quick": "pretty-quick --staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "2.0.2",
    "@semantic-release/git": "5.0.0",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.19.1",
    "eslint-index": "1.4.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-no-unsafe-innerhtml": "1.0.16",
    "eslint-plugin-optimize-regex": "1.1.5",
    "eslint-plugin-react": "7.8.2",
    "eslint-plugin-scanjs-rules": "0.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "0.14.3",
    "lint-staged": "7.1.2",
    "npm-run-all": "4.1.3",
    "nsp": "3.2.1",
    "prettier": "1.12.1",
    "pretty-quick": "1.6.0",
    "semantic-release": "15.4.1",
    "sort-package-json": "1.15.0"
  },
  "peerDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-no-unsafe-innerhtml": "^1.0.16",
    "eslint-plugin-optimize-regex": "^1.1.5",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/npm",
        "tarballDir": "release"
      },
      {
        "path": "@semantic-release/git",
        "assets": ["**"]
      }
    ],
    "publish": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/github",
        "assets": [
          {
            "path": "release/*.tgz",
            "label": "Package tarball"
          }
        ]
      }
    ]
  }
}
